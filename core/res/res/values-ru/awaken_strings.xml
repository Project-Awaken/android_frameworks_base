<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Button that copies the url to the log of a crashed application -->
    <string name="aerr_copy">Скопировать URL журнала ошибки</string>
    <string name="url_copy_success">URL успешно скопирован</string>
    <string name="url_copy_failed">Произошла ошибка при загрузке журнала в Memochō</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_fakePackageSignature">Подмена подписи приложения</string>

    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_fakePackageSignature">Позволяет приложению выдавать себя за другое приложение. Вредоносные приложения могут использовать это для доступа к данным частного приложения. Законное использование включает эмулятор, притворяющийся тем, что он имитирует. Предоставляйте это разрешение только с осторожностью!</string>

    <!-- Title of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgrouplab_fake_package_signature">Подмена подписи приложения</string>

    <!-- Description of a category of application permissions, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permgroupdesc_fake_package_signature">разрешить подмену подписи</string>

    <!-- Message shown to the user when the apps requests permission from this group. If ever possible this should stay below 80 characters (assuming the parameters takes 20 characters). Don't abbreviate until the message reaches 120 characters though. [CHAR LIMIT=120] -->
    <string name="permgrouprequest_fake_package_signature">Разрешить
        &lt;b><xliff:g id="app_name" example="Gmail">%1$s</xliff:g>&lt;/b> подменить подпись пакета?</string>

    <!-- Vendor mismatch fingerprint warning [CHAR LIMIT=NONE] -->
    <string name="system_error_vendorprint">Ваш образ вендора не соответствует системе. Пожалуйста, прошейте <xliff:g id="string">%s</xliff:g> образ вендора для вашего устройства</string>

    <!-- Global actions -->
    <string name="reboot_to_bootloader_title">Перезагрузка в загрузчик</string>
    <string name="reboot_to_bootloader_message">Перезагрузка в загрузчик\u2026</string>
    <string name="reboot_to_recovery_title">Перезагрузка в рекавери</string>
    <string name="reboot_to_recovery_message">Перезагрузка в рекавери\u2026</string>
    <string name="reboot_system_title">Перезапуск системы</string>
    <string name="reboot_system_message">Перезапуск системы\u2026</string>
